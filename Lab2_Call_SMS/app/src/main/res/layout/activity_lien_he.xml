<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAF8F8"
    tools:context=".LienHeActivity">

    <ImageButton
        android:id="@+id/imgfb"
        android:layout_width="251dp"
        android:layout_height="202dp"
        android:layout_gravity="center"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/fb"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteY="1dp" />

    <ImageButton
        android:id="@+id/imgdiscord"
        android:layout_width="217dp"
        android:layout_height="209dp"
        android:layout_gravity="center"
        android:background="#00000000"
        android:src="@drawable/discord"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgfb" />

    <ImageButton
        android:id="@+id/imggifhub"
        android:layout_width="367dp"
        android:layout_height="199dp"
        android:layout_gravity="center"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgdiscord"
        app:srcCompat="@drawable/gifhub" />

    <Button
        android:id="@+id/bntback4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Back"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imggifhub" />

</androidx.constraintlayout.widget.ConstraintLayout>